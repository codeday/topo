{
  "name": "@codeday/topo",
  "version": "9.1.1",
  "license": "Artistic-2.0",
  "files": [
    "dist",
    "Atom",
    "Molecule",
    "Organism",
    "Theme",
    "utils",
    "_utils"
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "Atom": [
        "dist/Atom/index.d.ts"
      ],
      "Molecule": [
        "dist/Molecule/index.d.ts"
      ],
      "Organism": [
        "dist/Organism/index.d.ts"
      ],
      "Theme": [
        "dist/Theme/index.d.ts"
      ],
      "utils": [
        "dist/utils.d.ts"
      ],
      "_utils": [
        "dist/_utils.d.ts"
      ],
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./Atom": {
      "types": "./dist/Atom/index.d.ts",
      "require": "./dist/Atom/index.js",
      "import": "./dist/Atom/index.mjs"
    },
    "./Molecule": {
      "types": "./dist/Molecule/index.d.ts",
      "require": "./dist/Molecule/index.js",
      "import": "./dist/Molecule/index.mjs"
    },
    "./Organism": {
      "types": "./dist/Organism/index.d.ts",
      "require": "./dist/Organism/index.js",
      "import": "./dist/Organism/index.mjs"
    },
    "./Theme": {
      "types": "./dist/Theme/index.d.ts",
      "require": "./dist/Theme/index.js",
      "import": "./dist/Theme/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "require": "./dist/utils.js",
      "import": "./dist/utils.mjs"
    },
    "./_utils": {
      "types": "./dist/_utils.d.ts",
      "require": "./dist/_utils.js",
      "import": "./dist/_utils.mjs"
    }
  },
  "scripts": {
    "build": "NODE_OPTIONS='--max-old-space-size=8192' yarn tsup --dts",
    "build:fast": "yarn tsup src",
    "trace": "tsc --traceResolution",
    "build:esm": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist/esm",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs && tscpaths -p tsconfig.json -s ./src -o ./dist/cjs",
    "gen:theme-typings": "chakra-cli tokens ./src/Theme/vars/index.tsx",
    "logos": "create-chakra-icons  -o src/Atom/Logo/Icons.ts ./src/Atom/Logo/svgs --typescript",
    "prepare": "yarn build"
  },
  "dependencies": {
    "@accessible/use-id": "^1.1.2",
    "@chakra-ui/cli": "^2.3.0",
    "@chakra-ui/react": "^2.5.1",
    "@chakra-ui/styled-system": "^2.6.1",
    "@codeday/topocons": "^2.0.1",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@tylermenezes/cognitoforms-react": "^2.0.13",
    "fathom-react": "^1.0.1",
    "framer-motion": "^4",
    "graphql": "^16.0.1",
    "graphql-request": "^3.6.1",
    "lodash.get": "^4.4.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-load-script": "^0.0.6",
    "recompose": "^0.30.0",
    "stylis": "~4.0.10",
    "swr": "^1.0.1"
  },
  "devDependencies": {
    "@types/lodash.get": "^4.4.6",
    "@types/node": "^16.11.6",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-pdf": "^5.0.9",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/recompose": "^0.30.9",
    "concurrently": "^6.4.0",
    "create-chakra-icons": "^1.0.3-dev.0",
    "cross-env": "^7.0.3",
    "tscpaths": "^0.0.9",
    "tsup": "^6.6.3",
    "typescript": "^4.5.2"
  },
  "tsup": {
    "entry": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!src/**/*.svg"
    ],
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ]
  }
}